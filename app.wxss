}
.article-list-box .article-list .text-box{
    display: flex;
    height: 150rpx;
    flex-direction: column;
    justify-content: space-between;
    width: 100%;
}
.article-list-box .article-list .text-box div{
    font-size: 24rpx;
    color: #999;
    margin-bottom: 30rpx;
}
.article-list-box .article-list .text-box h1{
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
    max-width: 100%;
}
.article-list-box .article-list .text-box p{
    font-size: 22rpx;
    color: #999;
}
.article-list-box .article-list .text-box p span{
    margin-right: 30rpx;
}