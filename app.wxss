.header-tab{
    position: relative;
}
.header-bg{
    position: absolute;
    height: 30rpx;
    width: 100%;
    left: 0;
    top: 0;
    background-color: #1684f6;
}
.header-tab-box{
    margin: 0 30rpx;
    background-color: #fff;
    display: flex;
    height: 88rpx;
    border-radius: 8rpx;
    position: relative;
    z-index: 1;
    box-shadow: 0rpx 4rpx 30rpx rgba(71, 158, 248, .2);
    align-items: center;
}
.header-tab-box view{
    flex: 10;
    height: 65rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    border-right: 1px  solid #eee;
}
.header-tab-box view:last-child{
    border: none;
}
.header-tab-box view span{
    height: 100%;
    display: inline-block;
    line-height: 65rpx;
    text-align: center;
    color: #999;
    position: relative;
}
.header-tab-box view span.aft{
    color: #333;
}
.header-tab-box view span.aft i{
    display: inline-block;
    width: 100%;
    height: 2rpx;
    background-color: #007EFF;
    position: absolute;
    bottom: -11.5rpx;
    left: 0;
}

.header-son-tab{
    margin-top: 40rpx;
    padding: 0 30rpx;
    display: flex;
    justify-content: space-between;
}

.header-son-tab span{
    height: 60rpx;
    width: 150rpx;
    border: 1px solid #eee;
    border-radius: 6rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #999;
    font-size: 28rpx;
}
.header-son-tab span.aft{
    color: #fff;
    background-color: #1684f6;
    border-color: #1684f6;
}

.header-tool{
    display: flex;
    justify-content: space-between;
    padding: 0 30rpx;
    margin-top: 30rpx;
    align-items: center;
}
.header-tool image{
